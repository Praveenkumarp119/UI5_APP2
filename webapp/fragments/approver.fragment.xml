<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
	    id="approver1"
		title="Approver"
		class="sapUiPopupWithPadding"
		items="{/results}"
		search="approverValueHelpSearch"
		confirm="approverValueHelpClose"
		cancel="approverValueHelpClose">
		<StandardListItem
			
			iconDensityAware="false"
			iconInset="false"
			title="{Zapprover}"
			description="{Zpurorg}"
		
		 />
	</SelectDialog>
</core:FragmentDefinition>